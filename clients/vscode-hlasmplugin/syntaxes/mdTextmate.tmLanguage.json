{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "MD TEXTMATE",
  "patterns": [
    {
      "include": "#general_mdtm_statement"
    }
  ],
  "repository": {
    "general_mdtm_statement": {
      "patterns": [
        {
          "injectionSelector": "L:text -comment",
          "begin": ".*graphql request",
          "end": "(?<=\"\"\")",
          "name": "constant.numeric.hlasm",
          "patterns": [
            {
              "begin": "^\\s*(\"\"\")",
              "end": "^\\s*(\"\"\")",
              "name": "support.type.hlasm",
              "patterns": [
                {
                  "contentName": "entity.name.function.hlasm",
                  "begin": "^\\s*(aaa)",
                  "end": "^\\s*(aaa)",
                  "name": "inner.aaa..constant.numeric.hlasm"
                },
                {
                  "match": "^.*$",
                  "name": "inner.constant.numeric.hlasm"
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "scopeName": "source.mdTextmate"
}